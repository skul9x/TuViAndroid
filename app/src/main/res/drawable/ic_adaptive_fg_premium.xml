<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Center Glow (Behind) -->
    <path
        android:pathData="M54,54m-25,0a25,25 0 1,1 50,0a25,25 0 1,1 -50,0">
        <aapt:attr name="android:fillColor">
            <gradient
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="25"
                android:type="radial">
                <item android:offset="0.0" android:color="#80FFD700" />
                <item android:offset="1.0" android:color="#00FFD700" />
            </gradient>
        </aapt:attr>
    </path>

    <!-- Base 8-Point Star Layer 1 (Darker Gold) -->
    <path
        android:pathData="M54,18 L63,45 L90,54 L63,63 L54,90 L45,63 L18,54 L45,45 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="45"
                android:type="radial">
                <item android:offset="0.3" android:color="#FFFFA000" />
                <item android:offset="1.0" android:color="#FFFF6F00" />
            </gradient>
        </aapt:attr>
    </path>

    <!-- Base 8-Point Star Layer 2 (Lighter Gold - Rotated) -->
    <!-- Creating the interwoven effect with a smaller offset star -->
    <path
        android:pathData="M54,22 L61,46 L86,54 L61,62 L54,86 L47,62 L22,54 L47,46 Z"
        android:strokeWidth="1"
        android:strokeColor="#FFFFD54F"/>

    <!-- Intricate Geo-Pattern: Interlocking Circular Arcs (The "Flower" effect) -->
    <path
        android:fillColor="#00000000"
        android:strokeWidth="0.8"
        android:strokeColor="#FFFFFF00"
        android:pathData="M54,34 Q74,34 74,54 Q74,74 54,74 Q34,74 34,54 Q34,34 54,34" />
    
    <!-- Main Diamond Shape (High Shine) -->
    <path
        android:pathData="M54,26 L60,54 L54,82 L48,54 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="54"
                android:endY="82"
                android:startX="54"
                android:startY="26"
                android:type="linear">
                <item android:offset="0.0" android:color="#FFFFECB3" />
                <item android:offset="0.5" android:color="#FFFFCA28" />
                <item android:offset="1.0" android:color="#FFFFECB3" />
            </gradient>
        </aapt:attr>
    </path>
    <!-- Horizontal part of Diamond -->
    <path
        android:pathData="M26,54 L54,60 L82,54 L54,48 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="82"
                android:endY="54"
                android:startX="26"
                android:startY="54"
                android:type="linear">
                <item android:offset="0.0" android:color="#FFFFECB3" />
                <item android:offset="0.5" android:color="#FFFFCA28" />
                <item android:offset="1.0" android:color="#FFFFECB3" />
            </gradient>
        </aapt:attr>
    </path>

    <!-- Central Gem/Core -->
    <path
        android:pathData="M54,54m-6,0a6,6 0 1,1 12,0a6,6 0 1,1 -12,0">
        <aapt:attr name="android:fillColor">
            <gradient
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="6"
                android:type="radial">
                <item android:offset="0.0" android:color="#FFFFFFFF" />
                <item android:offset="0.4" android:color="#FFFFD700" />
                <item android:offset="1.0" android:color="#FFF57F17" />
            </gradient>
        </aapt:attr>
    </path>
    
    <!-- Outer Decorative Ring -->
     <path
        android:fillColor="#00000000"
        android:strokeColor="#FFFFD700"
        android:strokeWidth="1.5"
        android:pathData="M54,54m-30,0a30,30 0 1,1 60,0a30,30 0 1,1 -60,0"/>

</vector>
